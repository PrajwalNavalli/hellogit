pipeline{
    agent any
    stages{
        stage("git clone"){
            steps{
              git url: 'https://github.com/openmrs/openmrs-core.git',
                  branch: 'master'
                }
            }
            stage('build'){
            steps{
              sh './mvn clean package'
              sh './mvn jetty:run'
            }
        }
    }}